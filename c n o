import java.io.*;
public class box {
	double width;
	double height;
	double depth;
	}
class boxdemo{
	public static void main(String[] args) {
		box mybox= new box();  ////object declaring
		//assign value
		mybox.width= 100;
		mybox.height= 200;
		mybox.depth=300;
		 
		vol=mybox.width*mybox.height*mybox.depth;
		System.out.println("volume is " +vol);
		
		
		// TODO Auto-generated method stub

	}

}

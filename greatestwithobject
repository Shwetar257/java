import java.util.Scanner;
class stat {
	int a,b,c;
	void method(int a,int b, int c) 
	{
	if(a>b && a>c)
		System.out.println("this value is greatest = "+a);
	if(b>a && b>c)
		System.out.println("this value is greatest = "+b);
	else 
		System.out.println("this value is greatest = "+c);
 }
	
}
public class greatestwithobj {

	public static void main(String[] args) {
		int a,b,c;
		Scanner oo = new Scanner(System.in);
		System.out.print("enter the value of A = ");
		a =oo.nextInt();
		System.out.print("enter the value of b= ");
		b= oo.nextInt();
		System.out.print("enter the value of C = ");
		c = oo.nextInt();
		
		stat s1=new stat();
		s1.method(a,b,c); 
				
		
		
		// TODO Auto-generated method stub
		

	}

}
